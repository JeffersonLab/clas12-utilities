#%Module1.0
proc ModulesHelp { } {
    puts stderr "This module sets up environment for evio"
}
module-whatis   "evio 5.1"

set version 5.1

set JLAB_ROOT /site/12gev_phys
set JLAB_VERSION 2.3
set OSRELEASE Linux_CentOS7.2.1511-x86_64-gcc4.8.5
set JLAB_SOFTWARE $JLAB_ROOT/$JLAB_VERSION/$OSRELEASE

prepend-path PATH $JLAB_SOFTWARE/evio/$version/bin

prepend-path LD_LIBRARY_PATH $JLAB_SOFTWARE/evio/$version/lib


#%Module1.0
proc ModulesHelp { } {
    puts stderr "This module sets up environment for clas12-workflow"
}
module-whatis   "clas12-workflow 0.3"

set d /group/clas12/packages/clas12-workflow/0.3

module load rcdb
module load root
#prereq rcdb
#prereq root

prepend-path PYTHONPATH $d/lib/util
prepend-path PYTHONPATH $d/lib/swif
prepend-path PYTHONPATH $d/lib/clas12

setenv CLAS12WFLOW $d

prepend-path PATH $d/scripts

if { [module-info mode load] } {
    puts stderr ">>>"
    puts stderr ">>> WARNING:  clas12-workflow model numbers and descriptions have changed."
    puts stderr ">>>           Check the -h help info."
    puts stderr ">>>"
}

